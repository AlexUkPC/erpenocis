    <div class="card">
      <div class="card-body">
        <div class="row mb-2">
          <div class="col-md-12">
            <p class="card-title fftstst">Facturi furnizori/Taxe salarii/Taxe stat/TVA de plata</p>
          </div>
        </div>
        <div class="row mb-4">
          <div class="col-md-12">
            <%= link_to "Urmatoarele 7 zile", root_path(sup_range: "7"), class: "btn btn-sm #{params[:sup_range]=="7" ? "btn-primary" : "btn-outline-primary"} btn-rounded " %>
            <%= link_to "Urmatoarele 15 zile", root_path(sup_range: "15"), class: "btn btn-sm #{params[:sup_range]=="15" ? "btn-primary" : "btn-outline-primary"} btn-rounded" %>
            <%= link_to "Urmatoarele 30 zile", root_path(sup_range: "30"), class: "btn btn-sm #{params[:sup_range]=="30" ? "btn-primary" : "btn-outline-primary"} btn-rounded" %>
            <%= link_to "Urmatoarele 45 zile", root_path(sup_range: "45"), class: "btn btn-sm #{params[:sup_range]=="45" ? "btn-primary" : "btn-outline-primary"} btn-rounded" %>
            <%= link_to "Urmatoarele 60 zile", root_path(sup_range: "60"), class: "btn btn-sm #{params[:sup_range]=="60" ? "btn-primary" : "btn-outline-primary"} btn-rounded" %>
          </div>
        </div>
        <div class="table-responsive">
          <% @total = 0 %>
          <table id="dashboardSuppliersTable" class="table" style="width:100%">
            <thead>
              <tr>
                <th>Furnizor/Taxe salarii/Taxe stat/TVA</th>
                <th>Suma</th>
              </tr>
            </thead>
            <tbody>
              <% @suppliers.each do |supplier| %> 
                <% @supplier_invoices = SupplierInvoice.where(supplier_id: supplier.id).between_due_dates(params[:sup_range] ? Date.today : params[:start_date] || Date.today-1.month, params[:sup_range] ? Date.today+params[:sup_range].to_i.days : params[:end_date] || Date.today) %>
                <% if @supplier_invoices.sum(:value) > 0 %>
                  <tr>
                    <td><%= supplier.name %></td>
                    <% @to_be_payed = @supplier_invoices.sum(:value) - SupplierInvoicePayment.where(supplier_invoice_id: @supplier_invoices.ids).sum(:paid_amount) %>
                    <% @total += @to_be_payed %>
                    <td><%= @to_be_payed %></td>
                  </tr>
                <% end %>
              <% end %>
              <% @employee_taxes = EmployeeSalary.between_due_dates(params[:sup_range] ? Date.today : params[:start_date] || Date.today-1.month, params[:sup_range] ? Date.today+params[:sup_range].to_i.days : params[:end_date] || Date.today).sum(:salary_tax) %>
              <% if @employee_taxes > 0 %>
                <tr>
                  <td>Taxe salarii</td>
                  <td><%= @employee_taxes %></td>
                  <% @total += @employee_taxes %>
                </tr>        
              <% end %>
              <% @expense_taxes_tva = ExpenseValue.where("vat_taxes = true").between_due_dates(params[:sup_range] ? Date.today : params[:start_date] || Date.today-1.month, params[:sup_range] ? Date.today+params[:sup_range].to_i.days : params[:end_date] || Date.today).sum(:value) %>
              <% if @expense_taxes_tva > 0 %>
                <tr>
                  <td>Taxe stat/TVA</td>
                  <td><%= @expense_taxes_tva %></td>
                  <% @total += @expense_taxes_tva %>
                </tr>        
              <% end %>
            </tbody>
            <tfoot>
              <tr>
                <th>Total</th>
                <th><%= @total %></th>
              </tr>
            </tfoot>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

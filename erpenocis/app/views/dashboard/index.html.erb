                  <td><%= order.ordered_quantity %></td>
                  <td><%= order.delivery_date %></td>
                  <td>
                    <%= link_to edit_order_path(order, sm: @start_month, sy: @start_year, em: @end_month, ey: @end_year), class: "btn" do %>
                      <i class="ti-pencil-alt btn-icon-prepend text-success"></i>
                    <% end %>
                  </td>

                      <th style="display: none">Sterge</th><%# 19 %>
                      <th></th><%# 20 %>
                    </tr>
                  </thead>
                  <tbody>
                    <% @orders.each do |order| %>
                      <tr>
                        <td><%= order.id %></td><%# 0 %>
                        <td><%= order.status.titleize %></td><%# 1 %>
                        <td><%= order.category %></td><%# 2 %>
                        <td><%= order.name_type_color %></td><%# 3 %>
                        <td><%= order.needed_quantity %></td><%# 4 %>
                        <td><%= order.unit %></td><%# 5 %>
                        <td><%= order.supplier_id ? @suppliers.find(order.supplier_id).name : "" %></td><%# 6 %>
                        <td><%= order.ordered_quantity %></td><%# 7 %>

                        <td style="display: none"><%= image_tag user_profile_picture(@users.find(order.user_id), 100) , alt: "profile" %></td><%# 8 %>
                        <td style="display: none"><%= @users.find(order.user_id).username %></td><%# 9 %>
                        <td style="display: none"><%= order.updated_at.strftime('%d/%m/%Y') %></td><%# 10 %>

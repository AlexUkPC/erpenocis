      var tr = $(this).closest('tr');
      var row = table_orders.row( tr );
      if ( row.child.isShown() ) {
          // This row is already open - close it
          row.child.hide();
          tr.removeClass('shown');
      }
      else {
          // Open this row
          row.child( format_orders(row.data(),tr.find("td:eq(8)").html(),tr.find("td:eq(9)").text(),tr.find("td:eq(11)").text(),tr.find("td:eq(12)").text(),tr.find("td:eq(13)").text(),tr.find("td:eq(14)").text(),tr.find("td:eq(16)").text(),tr.find("td:eq(17)").html(),tr.find("td:eq(18)").html(),tr.find("td:eq(19)").html()) ).show();
          tr.addClass('shown');
      }
    } );
  });
  

                        <td style="display: none"><%= order.delivery_date %></td><%# 14 %>
                        <td style="display: none"><%= order.obs %></td><%# 15 %>
                        <td style="display: none"><%= excerpt(order.obs, "", radius: 200) %></td><%# 16 %>
